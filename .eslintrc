{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "globals": {
    "think": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "import/extensions": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "no-unused-vars": 1,
    "no-underscore-dangle": 0,
    "no-unreachable-loop": 0
    "prefer-const": 0,
  }
}
